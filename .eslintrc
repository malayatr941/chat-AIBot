{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
         "@typescript-eslint/no-var-requires":0,
         "@typescript-eslint/ban-ts-comment":0,
         "@typescript-eslint/no-explicit-any":0,
         "no-useless-escape":0
    }
}
